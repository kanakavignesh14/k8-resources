apiVersion: v1
kind: Pod
metadata: 
  name: resourceCPU
  labels:  
     purpose: cpuutilization

spec:
   containers:
   - name: resource
     image: nginx
     resources:
        requests:
           memory: "68Mi"
           cpu: "100m" # "1\10th of cpu"
        # give limits cpu and memory more than request or equal    
        limits:
           memory: "100Mi"
           cpu: "120m"   
         
     