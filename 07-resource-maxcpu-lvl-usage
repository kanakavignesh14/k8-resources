apiVersion: v1
kind: Pod
metadata:
  name: resource-cpu
  labels:
    purpose: cpuutilization

spec:
  containers:
    - name: resource
      image: nginx
      resources:
        requests:
          memory: "68Mi"
          cpu: "100m"   # 1/10th of a CPU
        limits:
          memory: "100Mi"
          cpu: "120m"
